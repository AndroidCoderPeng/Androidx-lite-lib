<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--圆形头像框属性-->
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CircleProgressBar">
        <!--进度条底色-->
        <attr name="cpb_backgroundColor" format="color" />
        <!--进度条前景色-->
        <attr name="cpb_foregroundColor" format="color" />
        <!--文字-->
        <attr name="cpb_text" format="string" />
    </declare-styleable>
</resources>